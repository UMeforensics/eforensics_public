citHeader("To cite eforensics in publications use:")
citEntry(entry = "manual",
         title = "Election Forensics: Positive Empirical Models of Election Fraud",
         author = personList(as.person("Diogo Ferrari"), as.person("Walter Mebane"), as.person("Kevin McAlister"), as.person("Patrick Wu")),
         year = "2019",
         note = "R package version 0.0.0.9000 (Supported by NSF grant SES 1523355)",
         textVersion = paste("Mebane, W. and Ferrari, D. and McAlister, K. and Wu, P.", 
                             "(2019)",
                             "Election Forensics: Positive Empirical Models of Election Fraud.",
                             "R package version 0.0.0.9000",
                             "(Supported by NSF grant SES 1523355)")
         )

